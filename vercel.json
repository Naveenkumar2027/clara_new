{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "handle": "filesystem" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/appointment-frontend/(.*)", "dest": "/public/appointment-frontend/$1" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
